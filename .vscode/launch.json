{
	"version": "0.2.0",
	"configurations": [
        

		{
			"type": "extensionHost",
			"request": "launch",
			"name": "Launch Client",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}/vs_code_lsp/client"
			],
			"outFiles": [
				"${workspaceFolder}/vs_code_lsp/client/out/**/*.js"
			],
			"preLaunchTask": "npm: compile - client"
		}
	]
}
